{
  "name": "@wildpeaks/interpolators",
  "version": "1.1.0",
  "author": "Cecile Muller",
  "license": "MIT",
  "description": "Typescript classes to interpolate between keyframes in the style of VRML interpolators",
  "eslintConfig": {
    "extends": "@wildpeaks/typescript"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": true,
    "bracketSpacing": false
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "feat: update ${dependency}",
      "devDependencyUpdate": "chore: update ${dependency}"
    }
  },
  "main": "src/interpolators.ts",
  "typings": "src/interpolators.ts",
  "files": [
    "tsconfig.json",
    "src",
    "test"
  ],
  "scripts": {
    "test": "jasmine --helper=register.typescript.js test/*.spec.ts"
  },
  "repository": "https://github.com/wildpeaks/package-interpolators",
  "keywords": [
    "wildpeaks",
    "typescript",
    "interpolation"
  ],
  "bugs": {
    "url": "https://github.com/wildpeaks/package-interpolators/issues"
  },
  "homepage": "https://github.com/wildpeaks/package-interpolators#readme",
  "peerDependencies": {
    "typescript": "*"
  },
  "devDependencies": {
    "@types/jasmine": "3.5.0",
    "@types/node": "12.12.21",
    "@wildpeaks/eslint-config-typescript": "8.0.0-alpha7",
    "eslint": "6.7.2",
    "prettier": "1.19.1",
    "jasmine": "3.5.0",
    "ts-node": "8.5.4",
    "typescript": "3.7.3"
  }
}
